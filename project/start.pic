using "c/ffi/coo";
using "builtin/os";
using "builtin/syncio";

fn start() {
    say xy: struct.Coordinate = coo.create(10, 200);
    coo.dump(xy);

    // creating a dir with syscall
    os.mkdir("/workspaces/x-language/project/hello", os.S_IRUSR);

    syncio.printf(" value of nagaraj: %d \n", coo.nagaraj);
}