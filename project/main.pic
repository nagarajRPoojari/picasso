fn start() {

    asyncio.printf("Hello world\n");

    say y:int = 2;
    while y > 0 {
        say x:int = 10;
        while x > 0 {
            asyncio.printf("[%d, %d]\n", x, y);
            if(x == 3){
                break;
            }
            x = x - 1;
        }
        asyncio.printf(" === \n");
        y = y - 1;
    }
}