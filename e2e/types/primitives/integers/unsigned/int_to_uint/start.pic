using "builtin/syncio";
using "builtin/types";

class Test {
    fn Test() {}

    fn testIntToUInt() {
        // int8(-1) can to stored as uint8(255) but int64(-1) can't be stored 
        // because it's 2 complement can't be interpreted within uint8's boundary 
        // so below should work fine & print 255.
        say y: int8 = -1;
        say x: uint8 = y; 
        syncio.printf("value of x: %u , type:%s\n", x, types.type(x));
    }
}

fn start() {
    say t: start.Test = new start.Test();
    t.testIntToUInt();
}