using "builtin/syncio";
using "builtin/array";

using "utils";

class Utils {
    fn Utils() {}

    fn sum(a: int, b: int): int {
        return a + b;
    }
}

class Test {
    fn Test() {}

    fn testSelfModuleClassAccess() {
        say u: start.Utils = new start.Utils();
        say x: int = 10;
        say y: int = 20;
        syncio.printf("%d+%d=%d\n", x, y,u.sum(x, y));
    }

    fn testCrossModuleClassAccess() {
        say u: utils.Utils = new utils.Utils();
        say x: int = 10;
        say y: int = 20;
        syncio.printf("%d+%d=%d\n", x, y,u.sum(x, y));
    }
}


fn start() {
   say test: start.Test = new start.Test();

   test.testSelfModuleClassAccess();
   test.testCrossModuleClassAccess();
}